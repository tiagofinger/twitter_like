{
    "name": "twitter_like",
    "description": "Example using silex, mongoDB and phpunit.",
    "type": "api",
    "keywords": [
        "phpunit",
        "silex",
        "mongoDB",
        "restFul"
    ],
    "authors": [
        {
            "name": "Tiago Finger",
            "email": "tiagocfinger@gmail.com",
            "role": "lead"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "pimple/pimple": "~1.0",
        "saxulum/saxulum-doctrine-mongodb-odm-provider": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "saxulum/saxulum-console": "~2.2",
        "saxulum/saxulum-doctrine-mongodb-odm-commands": "dev-master@dev",
        "silex/silex": "~1.2",
        "symfony/doctrine-bridge": "~2.3",
        "symfony/form": "~2.3",
        "symfony/validator": "~2.3"
    },
    "suggest": {
        "saxulum/saxulum-console": "~2.2",
        "saxulum/saxulum-doctrine-mongodb-odm-commands": "dev-master@dev",
        "silex/silex": "~1.2",
        "symfony/doctrine-bridge": "~2.3",
        "symfony/form": "~2.3",
        "symfony/validator": "~2.3"
    },
    "autoload": {
        "psr-4": { 
            "Saxulum\\DoctrineMongodbOdmManagerRegistry\\": "src/",
            "MyApp\\": "src/MyApp/",
            "Documents\\": "src/MyApp/Documents/",
            "MyDB\\": "src/MyApp/Db/"
        }
    }
}